{
  "version": 2,
  "name": "eno-health-helper",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://eno.no1kmedi.com/api",
    "NEXT_PUBLIC_APP_NAME": "엔오건강도우미",
    "NEXT_PUBLIC_DOMAIN": "eno.no1kmedi.com"
  },
  "functions": {
    "frontend/next.config.js": {
      "maxDuration": 30
    }
  }
} 